  "If Statement": {
    "prefix": "if",
    "body": [
      "if ($1) {",
      "\t$0",
      "}"
    ],
    "description": "Create if statement"
  },
  "If-Else Statement": {
    "prefix": "ifelse",
    "body": [
      "if ($1) {",
      "\t$2",
      "} else {",
      "\t$0",
      "}"
    ],
    "description": "Create if-else statement"
  },
  "While Loop": {
    "prefix": "while",
    "body": [
      "while ($1) {",
      "\t$0",
      "}"
    ],
    "description": "Create while loop"
  },
  "For Loop": {
    "prefix": "for",
    "body": [
      "for ($1 in $2) {",
      "\t$0",
      "}"
    ],
    "description": "Create for-each loop"
  },
  "Struct": {
    "prefix": "struct",
    "body": [
      "struct ${1:Name} {",
